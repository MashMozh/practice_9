for A in range(1, 10):       # A не может быть 0, так как AB - двузначное
    for B in range(0, 10):
        AB = 10 * A + B
        result = AB * AB

        if 100 <= result <= 999:
            C = result // 100
            A2 = (result // 10) % 10
            B2 = result % 10

            if A2 == A and B2 == B:
                print(f"AB = {AB}")
                print(f"CAB = {result}")
                break
                
